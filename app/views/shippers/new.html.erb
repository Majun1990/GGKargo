<div class="booyah-box col-10 offset-1">
  <h1>New Shipper</h1>
  <%= simple_form_for @shipper do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :email %>
    <br/>
    <%= f.submit 'Create', class: 'btn btn-primary' %>
  <% end %>
</div>